#include <iostream>
using namespace std;
int main() {
	
	cout<<"Determining the biggest number among 3 numbers"<<endl;
	cout<<"====================================="<<endl;
	int smallest, biggest, num1, num2, num3;
	cout<<"\nInput the first number : ";
	cin>>num1;
	cout<<"Input the second number : ";
	cin>>num2;
	cout<<"Input the third number : ";
	cin>>num3;
	
	biggest=num1;
	
	if (num2 > biggest){
		biggest=num2;
	}
	if (num3 > biggest){
		biggest=num3;
	}
	cout<<"\nThe biggest number is "<<biggest<<endl;
	
	if (num1==num2){
	    cout<<"The first number is identical to the second number"<<endl;
	}
	if (num2==num3){
	    cout<<"The second number is identical to the third number"<<endl;
	}
	if (bil1==bil3){
	    cout<<"The first number is identical to the third number"<<endl;
	}

	return 0;	
}


